{
  "stepDefinitions": ["cypress/e2e/features/[filepart]/*.stepdef.js", "cypress/e2e/support/**/*.js"],
  "html": {
    "enabled": true,
    "output": "cypress/reports/html/index.html"
  },
  "messages": {
    "enabled": true,
    "output": "cypress/reports/messages/cucumber-messages.ndjson"
  },
  "json": {
    "enabled": true,
    "output": "cypress/reports/json/cucumber-report.json"
  },
  "filterSpecs": true,
  "omitFiltered": true
}
